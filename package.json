{
  "name": "front-end-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for front end projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itadakimas/front-end-boilerplate.git"
  },
  "author": "Itadakimas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itadakimas/front-end-boilerplate/issues"
  },
  "homepage": "https://github.com/itadakimas/front-end-boilerplate#readme",
  "dependencies": {
    "autoprefixer": "6.3.1",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "https://registry.npmjs.org/babel-preset-es2015/-/babel-preset-es2015-6.3.13.tgz",
    "bourbon": "4.2.6",
    "bourbon-neat": "1.7.4",
    "browser-sync": "2.10.0",
    "colors": "1.1.2",
    "del": "2.2.0",
    "errno": "0.1.4",
    "glob": "7.0.3",
    "gulp": "3.9.0",
    "gulp-imagemin": "2.4.0",
    "gulp-jade": "1.1.0",
    "gulp-jshint": "2.0.0",
    "gulp-postcss": "6.1.0",
    "gulp-replace-task": "0.11.0",
    "gulp-sass": "2.1.1",
    "gulp-svg-sprite": "1.2.16",
    "gulp-watch": "4.3.5",
    "html-loader": "0.4.0",
    "imagemin-jpeg-recompress": "4.3.0",
    "imagemin-pngquant": "4.2.0",
    "jshint": "2.8.0",
    "jshint-stylish": "2.1.0",
    "json-loader": "0.5.4",
    "json-server": "https://registry.npmjs.org/json-server/-/json-server-0.8.4.tgz",
    "normalize.css": "3.0.3",
    "rlite-router": "1.1.3",
    "run-sequence": "1.1.5",
    "svg4everybody": "2.0.3",
    "vue": "1.0.17",
    "webpack": "2.0.6-beta",
    "webpack-stream": "3.1.0",
    "wolfy87-eventemitter": "4.3.0",
    "yargs": "3.31.0"
  },
  "scripts": {
    "build:all": "npm run clean -- --mode=distributable && npm run build:preprod && npm run build:prod",
    "build:preprod": "node_modules/gulp/bin/gulp.js --gulpfile gulp/gulpfile.js build --env=pre-production --mode=distributable",
    "build:prod": "node_modules/gulp/bin/gulp.js --gulpfile gulp/gulpfile.js build --env=production --mode=distributable",
    "clean": "node_modules/gulp/bin/gulp.js --gulpfile gulp/gulpfile.js clean",
    "dev": "node_modules/gulp/bin/gulp.js --gulpfile gulp/gulpfile.js dev"
  }
}
